/**
 * Zone Objects.
*/

<% @objects.sort.map do |object, options|%>
object Zone <%= object.inspect -%> {
  <% if options['endpoints'] -%>
  endpoints = <%= options['endpoints'].inspect %>
  <% end -%>
  <% if options['parent'] -%>
  parent = <%= options['parent'].inspect %>
  <% end -%>
  <% if options['global'] -%>
  global = <%= options['global'].inspect %>
  <% end -%>
}

<% end -%>
