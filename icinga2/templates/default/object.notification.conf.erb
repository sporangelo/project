/**
 * Notification Objects.
*/

<% @objects.sort.map do |object, options|%>
<%= options['object_class'] -%> Notification <%= object.inspect -%> {
  <% if options['import'] -%>
  import <%= options['import'].inspect %>
  <% end -%>
  <% if options['host_name'] -%>
  host_name = <%= options['host_name'].inspect %>
  <% end -%>
  <% if options['service_name'] -%>
  service_name = <%= options['service_name'].inspect %>
  <% end -%>
  <% if options['users'] -%>
  users = <%= options['users'].inspect %>
  <% end -%>
  <% if options['user_groups'] && !options['user_groups'].empty? -%>
  user_groups = <%= options['user_groups'].sort.uniq.inspect %>
  <% end -%>
  <% if options['command'] -%>
  command = <%= options['command'].inspect %>
  <% end -%>
  <% if options['interval'] -%>
  interval = <%= options['interval'] %>
  <% end -%>
  <% if options['period'] -%>
  period = <%= options['period'].inspect %>
  <% end -%>
  <% if options['zone'] -%>
  zone = <%= options['zone'].inspect %>
  <% end -%>
  <% if options['types'] && !options['types'].empty? -%>
  types = [ <%= options['types'].join(', ') %> ]
  <% end -%>
  <% if options['states'] && !options['states'].empty? -%>
  states = [ <%= options['states'].join(', ') %> ]
  <% end -%>
  <%- if options['times'] && !options['times'].empty? -%>
  times = {
  <% options['times'].each do |var, value| %>
    <% if var && value -%>
    <%= var %> = <%= value.inspect %>
    <%- end -%>
  <%- end -%>
  }
  <%- end -%>
  <% if options['custom_vars'] -%>
  <% Hash[options['custom_vars'].sort].each do |var, value| -%>
  <% if var && value -%>
  vars.<%= var -%> = <%= value.inspect %>
  <% end -%>
  <% end -%>
  <% end -%>
}

<% end -%>
