/**
 * Endpoint Object.
*/

<% @objects.sort.map do |object, options|%>
object Endpoint <%= object.inspect -%> {
  <% if options['host'] -%>
  host = <%= options['host'].inspect %>
  <% end -%>
  <% if options['port'] -%>
  port = <%= options['port'].inspect %>
  <% end -%>
  <% if options['log_duration'] -%>
  log_duration = <%= options['log_duration'].inspect %>
  <% end -%>
}

<% end -%>
