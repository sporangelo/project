/**
 * TimePeriod Objects.
*/

<% @objects.sort.map do |object, options|%>
<%= options['object_class'] -%> TimePeriod <%= object.inspect -%> {
  <% if options['import'] -%>
  import <%= options['import'].inspect %>
  <% end -%>
  <% if options['display_name'] -%>
  display_name = <%= options['display_name'].inspect %>
  <% end -%>
  <% if options['ranges'] -%>
  ranges = {
  <% options['ranges'].each do |day, time| -%>
  <% if day && time -%>
    <%= day.inspect -%> = <%= time.inspect %>
  <% end -%>
  <% end -%>
  }
  <% end -%>
}

<% end -%>
